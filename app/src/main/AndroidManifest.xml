<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maple">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Maple">
        <activity android:name=".ParkingDetailActivity"></activity>
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".AddParkingActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginScreenActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/Theme.Maple.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
        <activity android:name=".MainActivity"/>
    </application>

</manifest>